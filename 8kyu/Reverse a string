/*
Complete the solution so that it reverses the string passed into it.

'world'  =>  'dlrow'
'word'   =>  'drow'

*/

//Answer:

function solution(str) {
 let turnArray = str.split('');
 let revArray = turnArray.reverse();
 let finalString = revArray.join(''); 
     
     
return finalString;     

}
